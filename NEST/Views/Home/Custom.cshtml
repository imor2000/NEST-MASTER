@model NEST.Models.ContentBody
@{
    Layout = "~/Views/Shared/_SiteMasterSubpageLayout.cshtml";
    ViewBag.Title = Model.Title;
}


<h1>@Html.DisplayFor(model => model.Title)</h1>

@Html.Raw(Model.BodyText)          

<iframe width="100%" height="600" src="@Model.MediaSourceURL" frameborder="0" scrolling="no" style="float:left; display:inline-block;padding-right:100px;margin-bottom:20px;"></iframe>





