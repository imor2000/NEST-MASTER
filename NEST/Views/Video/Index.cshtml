@model IEnumerable<NEST.Models.ContentBody>

@{
    ViewBag.Title = "Video";
    Layout = "~/Views/Shared/_SiteMasterSubpageLayout.cshtml";

}


<h1>@ViewBag.Title</h1>

@foreach (var item in Model) {
           
   
    <h2>@Html.DisplayFor(modelItem => item.Title)</h2>
    <p>@Html.Raw(item.BodyText)</p>



}
